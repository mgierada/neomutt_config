# HTML
text/html; w3m -I %{charset} -T text/html; copiousoutput;
text/plain; nvim %s

#PDFs
application/pdf; open -a Preview %s
# application/pdf; /usr/local/bin/zathura %s pdf

#Images
image/png; /usr/bin/imgcat %s
image/jpeg; /usr/bin/imgcat %s
# image/png; /usr/bin/feh %s
# image/jpeg; /usr/bin/feh %s
